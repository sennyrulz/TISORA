{
  "version": 2,
  "builds": [
    {
      "src": "frontEnd/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "frontEnd/dist"
      }
    },
    {
      "src": "backEnd/api/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/backEnd/api/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/frontEnd/dist/index.html"
    }
  ]
}
